{
	"name": "CSA2DWStaging_Mainpl1_copy1",
	"properties": {
		"activities": [
			{
				"name": "CSA_TableList",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "@concat('SELECT TABLE_SCHEMA,TABLE_NAME FROM ',pipeline().globalParameters.CSA_TransDB_Name,'.INFORMATION_SCHEMA.TABLES WHERE TABLE_TYPE = ''BASE TABLE''')",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "CSA_PreProdDB_ds",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEachCSATable",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "CSA_TableList",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('CSA_TableList').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "CSA2DW_DataCopy",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().Table_Schema}.@{item().Table_Name}"
								},
								{
									"name": "Destination",
									"value": "@{replace(item().Table_Schema,pipeline().globalParameters.Default_Schema,pipeline().globalParameters.Staging_Schema)}.@{item().Table_Name}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "SqlServerSink",
									"preCopyScript": {
										"value": "@{concat('Truncate Table ',replace(item().Table_Schema,pipeline().globalParameters.Default_Schema,pipeline().globalParameters.Staging_Schema),'.',item().Table_Name)}",
										"type": "Expression"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "CSA_PreProdDB_Table_parmtr_ds",
									"type": "DatasetReference",
									"parameters": {
										"csadb_Table_Name": {
											"value": "@item().Table_Name",
											"type": "Expression"
										},
										"csadb_Table_Schema": {
											"value": "@item().Table_Schema",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "Dev_DW_Table_Parmtr_ds",
									"type": "DatasetReference",
									"parameters": {
										"dwcsa_Table_Name": {
											"value": "@item().Table_Name",
											"type": "Expression"
										},
										"dwcsa_Table_Schema": {
											"value": "@replace(item().Table_Schema,pipeline().globalParameters.Default_Schema,pipeline().globalParameters.Staging_Schema)",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "Staging2DW_Copy",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "ForEachCSATable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "Staging2DW_Subpl1",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			}
		],
		"folder": {
			"name": "CSA_UAT_Pipelines/Ver.11182020"
		},
		"annotations": [
			"CSA"
		],
		"lastPublishTime": "2020-12-05T00:17:03Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}