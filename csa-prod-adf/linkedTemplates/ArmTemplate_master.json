{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "csa-prod-adf"
		},
		"ADLS_csaprodstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_csaprodstorage'"
		},
		"DTCPRODSQL01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DTCPRODSQL01'"
		},
		"DTCPRODSQL01_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DTCPRODSQL01'"
		},
		"ADLS_csaprodstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csaprodstorage.dfs.core.windows.net"
		},
		"DTCPRODSQL01_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nmsc\\asivakumar"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "csa-prod-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_csaprodstorage_accountKey": {
						"value": "[parameters('ADLS_csaprodstorage_accountKey')]"
					},
					"DTCPRODSQL01_connectionString": {
						"value": "[parameters('DTCPRODSQL01_connectionString')]"
					},
					"DTCPRODSQL01_password": {
						"value": "[parameters('DTCPRODSQL01_password')]"
					},
					"ADLS_csaprodstorage_properties_typeProperties_url": {
						"value": "[parameters('ADLS_csaprodstorage_properties_typeProperties_url')]"
					},
					"DTCPRODSQL01_properties_typeProperties_userName": {
						"value": "[parameters('DTCPRODSQL01_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}